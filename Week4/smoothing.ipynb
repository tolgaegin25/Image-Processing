   "source": [
    "import cv2\n",
    "import numpy as np\n",
    "path = r'lion.jpg'\n",
    "img = cv2.imread(path)\n",
    "\n",
    "im1 = cv2.blur(img,(5,5))\n",
    "im2 = cv2.boxFilter(img, -1,(2,2), normalize=True)\n",
    "\n",
    "cv2.imshow('image',np.hstack((im1,im2)))\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()\n",
    "cv2.waitKey(1)"
   ]
